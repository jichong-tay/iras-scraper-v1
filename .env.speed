# Speed Optimization Environment Variables for IRAS Scraper

# Browser Speed Settings
HEADLESS=false
WINDOW_SIZE=1280,720

# Reduced Timeouts (Major Speed Improvement)
IMPLICIT_WAIT=3
PAGE_LOAD_TIMEOUT=10
ELEMENT_WAIT_TIMEOUT=5

# Minimal Delays (Aggressive Speed)
REQUEST_DELAY=0.2
NAVIGATION_DELAY=0.5

# ReCAPTCHA Speed Settings
RECAPTCHA_MAX_RETRIES=1
RECAPTCHA_AUTO_SOLVE=true

# Logging (Reduced for Speed)
LOG_LEVEL=WARNING

# File Paths
INPUT_EXCEL_PATH=data/input_uens.xlsx
OUTPUT_EXCEL_PATH=data/output_results.xlsx
LOG_FILE=logs/scraper.log

# Performance Tweaks
DISABLE_IMAGES=true
DISABLE_CSS=true
AGGRESSIVE_CACHING=true